{
  "name": "@janiscommerce/api",
  "version": "6.5.1",
  "description": "A package for managing API from any origin",
  "main": "lib/index.js",
  "scripts": {
    "test": "export TEST_ENV=true; mocha --exit -R nyan --recursive tests/",
    "watch-test": "export TEST_ENV=true; mocha --exit -R nyan -w --recursive tests/",
    "test-ci": "nyc --reporter=lcov --reporter=text-summary mocha --exit --recursive tests/",
    "coverage": "nyc npm test",
    "lint": "eslint lib/ tests/",
    "build-types": "tsc lib/index.js --declaration --allowJs --emitDeclarationOnly --outDir types",
    "prepare": "husky install"
  },
  "files": [
    "lib/",
    "types/"
  ],
  "types": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janis-commerce/api.git"
  },
  "author": "Janis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/janis-commerce/api/issues"
  },
  "homepage": "https://github.com/janis-commerce/api#readme",
  "devDependencies": {
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.25.3",
    "husky": "^7.0.4",
    "mocha": "^9.2.0",
    "mock-require": "^3.0.3",
    "nyc": "^15.1.0",
    "sinon": "^13.0.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@janiscommerce/api-session": "^3.2.0",
    "@janiscommerce/events": "^0.1.0",
    "@janiscommerce/log": "^3.5.0",
    "@janiscommerce/superstruct": "^1.2.0",
    "lodash.omit": "4.5.0",
    "uuid": "^8.3.2"
  }
}
